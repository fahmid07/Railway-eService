<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="510.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InsertTrainController">
   <children>
      <ImageView fitHeight="510.0" fitWidth="706.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/bg.jpg" />
         </image>
      </ImageView>
      <Text fill="#2f5597" layoutX="292.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add a New Train">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TextField fx:id="trainNameText" layoutX="273.0" layoutY="101.0" prefHeight="40.0" prefWidth="186.0" promptText="Train Name" />
      <TextField fx:id="startTimeText" layoutX="273.0" layoutY="288.0" prefHeight="40.0" prefWidth="186.0" promptText="Start Time" />
      <TextField fx:id="endTimeText" layoutX="273.0" layoutY="344.0" prefHeight="40.0" prefWidth="186.0" promptText="End Time" />
      <Button layoutX="320.0" layoutY="419.0" mnemonicParsing="false" onAction="#SaveButton" prefHeight="40.0" prefWidth="89.0" text="SAVE" />
      <Button layoutX="67.0" layoutY="69.0" mnemonicParsing="false" onAction="#BackButton" prefHeight="32.0" prefWidth="74.0" text="Back" textFill="#2f5597">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ComboBox fx:id="fromStationBox" layoutX="275.0" layoutY="161.0" prefHeight="40.0" prefWidth="186.0" promptText="From Station" />
      <ComboBox fx:id="toStationBox" layoutX="275.0" layoutY="224.0" prefHeight="40.0" prefWidth="186.0" promptText="To Station" />
   </children>
</AnchorPane>
