      <Button layoutX="309.0" layoutY="395.0" mnemonicParsing="false" onAction="#SaveButton" prefHeight="40.0" prefWidth="89.0" text="SAVE" />
      <Button layoutX="77.0" layoutY="73.0" mnemonicParsing="false" onAction="#BackButton" prefHeight="32.0" prefWidth="74.0" text="Back" textFill="#2f5597">
